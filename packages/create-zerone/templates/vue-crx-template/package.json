{
  "name": "your-name",
  "displayName": "中文名称",
  "version": "0.0.1",
  "description": "中文名称",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "npm run clear && cross-env NODE_ENV=development run-p dev:*",
    "dev:prepare": "esno scripts/prepare.ts",
    "dev:background": "npm run build:background -- --mode development",
    "dev:web": "vite --config ./scripts/vite.popup.config.ts",
    "dev:js": "npm run build:js -- --mode development",
    "dev:server": "tsx ./scripts/server.ts",
    "build": "cross-env NODE_ENV=production run-s clear build:web build:prepare build:background build:js && npm run build:zip",
    "build:prepare": "esno scripts/prepare.ts",
    "build:background": "vite build --config ./scripts/vite.background.config.ts",
    "build:web": "vite build --config ./scripts/vite.popup.config.ts",
    "build:js": "vite build --config ./scripts/vite.content.config.ts",
    "build:zip": "tsx ./scripts/buildZip.ts",
    "pack:zip": "npm run build && rimraf third-gather-crx.zip && jszip-cli add extension/* -o ./third-gather-crx.zip",
    "clear": "rimraf --glob extension/dist extension/manifest.json extension.*",
    "api:gass": "zerone api -d -p ./src/api/gassApi",
    "tree": "treee -l 4 --ignore 'node_modules/, .git/, dist/, playground/'",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --max-warnings 0 --cache",
    "lint:fix": "pnpm run lint --fix"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@types/lodash-es": "^4.17.12",
    "@vespaiach/axios-fetch-adapter": "^0.3.1",
    "@vueuse/core": "^11.2.0",
    "@vueuse/integrations": "^11.2.0",
    "archiver": "^7.0.1",
    "axios": "^1.7.7",
    "element-plus": "2.8.3",
    "giime": "^0.4.4",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
    "pinia": "^2.2.6",
    "sass": "~1.78.0",
    "vue": "^3.5.12",
    "vue-clipboard3": "^2.0.0",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/runtime": "^7.26.0",
    "@ffflorian/jszip-cli": "3.5.0",
    "@iconify-json/ep": "^1.2.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@tsconfig/node18": "^18.2.4",
    "@types/archiver": "^6.0.3",
    "@types/chrome": "^0.0.280",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.9.0",
    "@types/serve-index": "^1.9.4",
    "@vitejs/plugin-vue": "^5.1.5",
    "@vue/tsconfig": "^0.6.0",
    "@zeronejs/eslint-config-vue3": "^0.0.7",
    "autoprefixer": "^10.4.20",
    "chokidar": "^4.0.1",
    "core-js": "^3.39.0",
    "cross-env": "^7.0.3",
    "eslint": "~8.57.1",
    "eslint-plugin-prettier": "^5.2.1",
    "esno": "^0.17.0",
    "express": "^4.21.1",
    "fs-extra": "^11.2.0",
    "kolorist": "^1.8.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "rimraf": "^5.0.10",
    "serve-index": "^1.9.1",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "~5.6.3",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-icons": "^0.20.1",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.11",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^2.1.10"
  }
}
