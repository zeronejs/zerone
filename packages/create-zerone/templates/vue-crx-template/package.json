{
  "name": "crx-demo",
  "displayName": "中文名称",
  "version": "0.0.1",
  "description": "中文名称",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "npm run clear && cross-env NODE_ENV=development run-p dev:*",
    "dev:prepare": "esno scripts/prepare.ts",
    "dev:background": "npm run build:background -- --mode development",
    "dev:web": "vite --config ./scripts/vite.popup.config.ts",
    "dev:js": "npm run build:js -- --mode development",
    "dev:server": "tsx ./scripts/server.ts",
    "build": "cross-env NODE_ENV=production run-s clear build:web build:prepare build:background build:js && npm run build:zip",
    "build:prepare": "esno scripts/prepare.ts",
    "build:background": "vite build --config ./scripts/vite.background.config.ts",
    "build:web": "vite build --config ./scripts/vite.popup.config.ts",
    "build:js": "vite build --config ./scripts/vite.content.config.ts",
    "build:zip": "tsx ./scripts/buildZip.ts",
    "pack:zip": "npm run build && rimraf third-gather-crx.zip && jszip-cli add extension/* -o ./third-gather-crx.zip",
    "clear": "rimraf --glob extension/dist extension/manifest.json extension.*",
    "api:gass": "zerone api -d -p ./src/api/gassApi",
    "ai-md": "zerone ai-md",
    "tree": "treee -l 4 --ignore 'node_modules/, .git/, dist/, playground/'",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --max-warnings 0 --cache",
    "lint:fix": "pnpm run lint --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@types/lodash-es": "^4.17.12",
    "@vespaiach/axios-fetch-adapter": "^0.3.1",
    "@vueuse/core": "^11.3.0",
    "@vueuse/integrations": "^11.3.0",
    "archiver": "^7.0.1",
    "axios": "^1.12.2",
    "element-plus": "2.9.10",
    "giime": "^0.7.15",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
    "pinia": "^2.3.1",
    "sass": "~1.90.0",
    "vue": "^3.5.21",
    "vue-clipboard3": "^2.0.0",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/runtime": "^7.28.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@ffflorian/jszip-cli": "3.5.0",
    "@iconify-json/ep": "^1.2.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@tsconfig/node18": "^18.2.4",
    "@types/archiver": "^6.0.3",
    "@types/chrome": "^0.0.280",
    "@types/express": "^5.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.18.4",
    "@types/serve-index": "^1.9.4",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/tsconfig": "^0.6.0",
    "@zeronejs/eslint-config-vue3": "^0.1.9",
    "autoprefixer": "^10.4.21",
    "chokidar": "^4.0.3",
    "core-js": "^3.45.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.35.0",
    "esno": "^0.17.0",
    "express": "^4.21.2",
    "fs-extra": "^11.3.2",
    "husky": "^9.1.7",
    "kolorist": "^1.8.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rimraf": "^5.0.10",
    "serve-index": "^1.9.1",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "~5.6.3",
    "unplugin-auto-import": "^0.19.0",
    "unplugin-icons": "^0.22.0",
    "unplugin-vue-components": "^0.28.0",
    "vite": "^6.3.6",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^2.2.12"
  }
}
