    "electron": "^38.3.0",
    "eslint": "^9.34.0",
    "git-cz": "^4.9.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.5"
  }
