{
  "name": "Electron",
  "productName": "Electron",
  "version": "1.0.0",
  "description": "an electron app",
  "main": ".vite/build/main.js",
  "type": "module",
  "scripts": {
    "dev": "electron-forge start",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:win": "chcp 65001 && electron-forge make --platform=win32",
    "make:mac": "electron-forge make --platform=darwin",
    "publish": "electron-forge publish",
    "build": "vite build --config ./vite.web.config.ts",
    "font": "zerone font_grabber -p ./src/assets/iconfont",
    "api": "zerone api -d -p ./src/api/demo",
    "ai-md": "zerone ai-md",
    "clean:pkg": "rimraf node_modules",
    "commit": "git-cz",
    "lint:fix": "pnpm run lint --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": "Giikin",
  "license": "MIT",
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "electron-winstaller"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@vueuse/core": "^13.7.0",
    "@vueuse/integrations": "^13.7.0",
    "axios": "^1.11.0",
    "dayjs": "^1.11.13",
    "dompurify": "^3.3.0",
    "echarts": "^5.6.0",
    "electron-squirrel-startup": "^1.0.1",
    "element-plus": "2.9.10",
    "giime": "^0.7.24",
    "highlight.js": "^11.11.1",
    "js-cookie": "^3.0.5",
    "marked": "^16.4.1",
    "pinia": "^3.0.3",
    "sass": "^1.90.0",
    "uuid": "^13.0.0",
    "vue": "^3.5.19",
    "vue-clipboard3": "^2.0.0",
    "vue-router": "^4.5.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@zeronejs/cli": "^3.8.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "tailwindcss": "^3.4.17",
    "jsdom": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "typescript": "^5.9.2",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-icons": "^22.2.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^6.3.5",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^3.2.4",
    "husky": "^9.1.7",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^24.8.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/js-cookie": "^3.0.6",
    "vue-tsc": "^2.2.12"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
